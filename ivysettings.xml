<ivysettings>
<settings defaultResolver="ivyrep"/>
<resolvers>
  <chain name="ivyrep" returnFirst="true">
  <ibiblio name="reactor-repo" root="${reactor.repo}"
           m2compatible="true"
           checkmodified="true"
           pattern="[organisation]/[module]/[revision]/[artifact]-[revision](-[classifier]).[ext]"/>
  <ibiblio name="cloudera-ext-release" root="https://repository.cloudera.com/artifactory/ext-release-local"
           m2compatible="true"
           checkmodified="true"
           pattern="[organisation]/[module]/[revision]/[artifact]-[revision](-[classifier]).[ext]"/>
    <ibiblio name="ibiblio" m2compatible="true"/>
  </chain>
  <filesystem name="tools" local="true">
    <artifact pattern="${java.home}/lib/tools.[type]" />
    <artifact pattern="${java.home}/../lib/tools.[type]" />
    <artifact pattern="${java.home}/../Classes/classes.[type]" />
  </filesystem>
</resolvers>
<modules>
  <module organisation="com.sun" name="tools" resolver="tools"/>
</modules>
</ivysettings>
